{
    "manifest_version": 2,
  
    "name": "SplitUp! - Tab manager",
    "description": "Control your tabs with this ultimate tab manager. Fast, user-friendly, secure and weekly updates!",
    "version": "1.3.1",
    "short_name" : "SplitUp!",
  
    "browser_action": {
      "default_icon": "split.png",
      "default_popup": "popup.html",
      "default_title": "SplitUp!"
      
    },
    "icons":
    {
       "128": "split.png"
    },

    "permissions": [
      "tabs"
    ]
  }